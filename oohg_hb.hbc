incpaths=include
libpaths=lib
incpaths=${hb_curdir}
mt=yes
gui=yes

# oohg libs
libs=oohg_main oohg_hbprinter oohg_miniprint oohg_bostaurus

# harbour's contrib libs
# hbhpdf must precede hbwin and xhb
hbcs=hbct hbhpdf hbmemio hbmisc hbmzip hbodbc hbtip hbzebra hbziparc rddsql sddodbc hbwin xhb

# mingw extra libs
{mingw}libs=vfw32

